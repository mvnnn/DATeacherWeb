<!DOCTYPE html>
<html>
<head>
  <title>Repository</title>
  <!-- including the header information -->
<% include Head.ejs %>
</head>

<body>
  <!-- including the navigation bar which appears after selecting the course -->
<% include StudentNavbar.ejs %>
<br><br>

<!-- script to make the current tab active -->
<script>
var x = document.getElementById("repository");
x.className = "active";
</script>


<!-- repository window in which the professor can disapprove the link if its not valid -->
          <div id="repos" class="w3-box-home">
            <% if(data) {%>
              <% for(var i=0; i < data.length; i++) { %>
                <form method="post">
                 <ul class="w3-ul w3-card-4">
                      <input type="hidden" value="<%= data[i].text %>" name="text">
                     <li name="del">@<%= data[i].course %>: <%= data[i].text %> <input type="submit" value="Delete" class="w3-btn w3-ripple w3-red"/></li>
                </ul>
              </form>
              <% } %>
              <% } %>
</div>
<br><br><br><br>

<!-- including the footer information -->
<% include Footer.ejs %>

</body>
</html>
